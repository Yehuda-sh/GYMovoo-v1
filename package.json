    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-native": "^5.0.0",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "prettier": "^3.3.2",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.38.0"
  },
  "keywords": [
