{
  "name": "gymovoo",
  "version": "1.0.0",
  "description": "GYMovoo - Smart Fitness App | אפליקציית כושר חכמה",
  "main": "index.ts",
  "scripts": {
    "start": "npx expo start",
    "start:dev-client": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "build": "expo build",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "check:health": "node scripts/projectHealthCheck.js",
    "check:quality": "node scripts/codeQualityCheck.js",
    "check:performance": "node scripts/performanceCheck.js",
    "check:security": "node scripts/securityCheck.js",
    "check:accessibility": "node scripts/accessibilityCheck.js",
    "check:navigation": "node scripts/checkNavigation.js",
    "check:components": "node scripts/checkMissingComponents.js",
    "check:data-flow": "node scripts/testDataFlow.js",
    "check:equipment": "node scripts/equipmentManager.js",
    "check:equipment:quick": "node scripts/equipmentManager.js --quick",
    "check:demo-user": "node testRealisticUserQuestionnaireFix.js",
    "check:demo-user:email": "node testRealisticUserFix.js",
    "check:questionnaire": "node testQuestionnaireLogic.js",
    "check:questionnaire:flow": "node testQuestionnaireFlow.js",
    "check:questionnaire:detection": "node testQuestionnaireDetection.js",
    "fix:equipment": "node scripts/equipmentFixer.js",
    "fix:equipment:interactive": "node scripts/equipmentFixer.js --interactive",
    "diagnose:questionnaire": "node scripts/questionnaireDiagnostic.js",
    "check:scroll": "node scripts/checkScrollIssues.js",
    "debug:bodyweight": "node scripts/debugBodyweightOptions.js",
    "check:all": "npm run check:health && npm run check:quality && npm run check:performance && npm run check:security && npm run check:accessibility && npm run check:equipment && npm run check:demo-user && npm run check:demo-user:email && npm run check:questionnaire && npm run check:questionnaire:flow && npm run check:questionnaire:detection",
    "lint": "npx eslint src/ --fix",
    "lint:check": "npx eslint src/",
    "type-check": "npx tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "clean": "rmdir /s /q node_modules && npm install",
    "clean:unix": "rm -rf node_modules && npm install",
    "audit": "npm audit",
    "audit:fix": "npm audit fix"
  },
